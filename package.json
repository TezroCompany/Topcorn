{
  "name": "topcorn",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT License",
  "scripts": {
    "prepare": "husky install",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run --network hardhat ./scripts/deploy.js",
    "test": "npx hardhat test --network hardhat",
    "deploy:testnetBNB": "npx hardhat run --network testnetBNB ./scripts/deploy.js",
    "verify:testnetBNB": "npx hardhat --network testnetBNB etherscan-verify --solc-input",
    "deploy:mainnetBNB": "npx hardhat run --network mainnetBNB ./scripts/deploy.js",
    "verify:mainnetBNB": "npx hardhat verify --network mainnetBNB",
    "clean": "npx hardhat clean",
    "format": "prettier --write \"tests/**/*.ts\" \"tasks/**/*.ts\" \"contracts/**/*.sol\" \"hardhat.config.ts\"",
    "pretty-quick": "pretty-quick --staged",
    "deploy:ropsten": "hardhat --network ropsten deploy",
    "deploy:ropsten:new": "hardhat --network ropsten deploy --reset",
    "verify:ropsten": "hardhat --network ropsten etherscan-verify --solc-input",
    "export:ropsten": "hardhat --network ropsten export --export deployments/ropsten.json",
    "release": "standard-version --commit-all",
    "first-release": "standard-version --commit-all --first-release"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "1.4.8",
    "@nomiclabs/hardhat-ethers": "2.0.5",
    "@nomiclabs/hardhat-etherscan": "2.1.8",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "4.6.0",
    "@openzeppelin/contracts-upgradeable": "4.4.2",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "7.0.1",
    "@typechain/hardhat": "2.3.0",
    "@types/chai": "4.2.21",
    "@types/mocha": "9.0.0",
    "@types/node": "16.9.0",
    "@uniswap/lib": "^1.1.2",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "*",
    "bignumber": "^1.1.0",
    "chai": "4.3.4",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.5.2",
    "hardhat": "2.9.1",
    "hardhat-abi-exporter": "2.4.0",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "0.11.4",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "7.0.4",
    "json-bigint": "^1.0.0",
    "prettier": "2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "pretty-quick": "3.1.3",
    "solidity-coverage": "^0.7.16",
    "standard-version": "^9.3.0",
    "ts-generator": "0.1.1",
    "ts-node": "10.2.1",
    "typechain": "5.1.2",
    "typescript": "4.4.2",
    "web3": "^1.7.4"
  }
}
